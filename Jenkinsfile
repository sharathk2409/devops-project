pipeline{
    agent any
    stages{
        stage('Hello World'){
            steps{
                sh 'echo "Hello for scm job!!!"'
            }
        }
        stage('Hello Jenkins'){
            steps{
                sh 'echo "Hello for jenkins!!!"'
            }
        }
        stage('Hello Jenkins'){
            steps{
                sh 'echo "Git sync up!!!"'
            }
        }
    }
}